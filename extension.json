{
	"name": "OpenID Connect",
	"version": "6.0-dev",
	"author": [
		"[https://www.mediawiki.org/wiki/User:Cindy.cicalese Cindy Cicalese]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:OpenID_Connect",
	"descriptionmsg": "openidconnect-desc",
	"type": "other",
	"license-name": "MIT",
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"extensions": {
			"PluggableAuth": ">= 6.0"
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\OpenIDConnect\\": "includes/"
	},
	"AutoloadClasses": {
		"MigrateOIDCSubjectAndIssuerFromUserTable":
			"maintenance/MigrateOIDCSubjectAndIssuerFromUserTable.php"
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"MessagesDirs": {
		"OpenIDConnect": [
			"i18n"
		]
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\OpenIDConnect\\OpenIDConnectHooks::loadExtensionSchemaUpdates",
		"PluggableAuthPopulateGroups": "MediaWiki\\Extension\\OpenIDConnect\\OpenIDConnectHooks::populateGroups"
	},
	"config": {
		"OpenIDConnect_MigrateUsersByEmail": {
			"value": false
		},
		"OpenIDConnect_MigrateUsersByUserName": {
			"value": false
		},
		"OpenIDConnect_ForceLogout": {
			"value": false
		},
		"OpenIDConnect_UseRealNameAsUserName": {
			"value": false
		},
		"OpenIDConnect_UseEmailNameAsUserName": {
			"value": false
		}
	},
	"attributes": {
		"PluggableAuth": {
			"OpenIDConnect": {
				"class": "MediaWiki\\Extension\\OpenIDConnect\\OpenIDConnect",
				"services": [
					"MainConfig",
					"AuthManager",
					"OpenIDConnectStore"
				]
			}
		}
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
